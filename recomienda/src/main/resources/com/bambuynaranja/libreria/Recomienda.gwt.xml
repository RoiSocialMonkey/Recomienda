<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='Recomienda'>
  	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	<inherits name="com.google.gwt.http.HTTP"/>
	<inherits name="com.google.gwt.logging.Logging" />

	<inherits name="com.google.gwt.i18n.I18N" />
	<extend-property name="locale" values="es" />
	<extend-property name="locale" values="en" />
	<set-property-fallback name="locale" value="es" />
  
  
  
  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <!--<inherits name='com.google.gwt.user.theme.standard.Standard' />-->
  <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

  <!-- Other module inherits                                      -->
	<!-- Other module inherits -->
	<inherits name="com.google.gwt.activity.Activity" />
	<inherits name="com.google.gwt.place.Place" />
	<inherits name="com.google.gwt.resources.Resources" />

	<!-- Bootstrap -->
	<inherits name="org.gwtbootstrap3.GwtBootstrap3NoTheme" />
		
	<!-- GWT JACKSON -->
	<inherits name="com.github.nmorel.gwtjackson.GwtJackson" />
	
		
  <!-- Specify the app entry point class.                         -->
  <entry-point class='com.bambuynaranja.libreria.client.Recomienda' />

  <!-- Specify the paths for translatable code                    -->
  <source path='client' />
  
  <!-- Configuracion del Logging -->
  <set-property name="gwt.logging.enabled" value="TRUE" />
  <set-property name="gwt.logging.logLevel" value="INFO" />
	<!-- Logs to Firebug -->
	<!--<set-property name="gwt.logging.firebugHandler" value="DISABLED" /> -->
	<!-- Remote Logging -->
	<set-property name="gwt.logging.simpleRemoteHandler" value="DISABLED" />
	<!-- Logs to the javascript console, which is used by Firebug Lite (for 
		IE), Safari and Chrome(?) -->
	<set-property name="gwt.logging.consoleHandler" value="ENABLED" />
	<!-- Logs by calling method GWT.log. These messages can only be seen in 
		Development mode — look for them in the DevMode window -->
	<set-property name="gwt.logging.developmentModeHandler"
		value="ENABLED" />
	<!-- Logs to the popup which appears in the upper left hand corner -->
	<!-- <set-property name="gwt.logging.popupHandler" value="DISABLED" /> -->
	<!-- Logs to stdout. These messages can only be seen in Development Mode 
		— look for them in the DevMode window -->
	<set-property name="gwt.logging.systemHandler" value="ENABLED" />
  

</module>
